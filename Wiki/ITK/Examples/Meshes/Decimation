	std<span class="sy4">::</span><span class="kw3">cout</span> <span class="sy1">&lt;&lt;</span> <span class="st0">&quot;Output file: &quot;</span> <span class="sy1">&lt;&lt;</span> OutputFilename <span class="sy1">&lt;&lt;</span> std<span class="sy4">::</span><span class="me2">endl</span><span class="sy4">;</span>
&#160;
	std<span class="sy4">::</span><span class="me2">string</span> strTriangleCount <span class="sy1">=</span> argv<span class="br0">&#91;</span><span class="nu0">3</span><span class="br0">&#93;</span><span class="sy4">;</span>
	std<span class="sy4">::</span><span class="me2">stringstream</span> ssTriangleCount<span class="sy4">;</span>
	ssTriangleCount <span class="sy1">&lt;&lt;</span> strTriangleCount<span class="sy4">;</span>
	<span class="kw4">unsigned</span> <span class="kw4">int</span> TriangleCount<span class="sy4">;</span>
	ssTriangleCount <span class="sy1">&gt;&gt;</span> TriangleCount<span class="sy4">;</span>
&#160;
	<span class="kw4">typedef</span> itk<span class="sy4">::</span><span class="me2">VTKPolyDataReader</span><span class="sy1">&lt;</span> MeshType <span class="sy1">&gt;</span>  ReaderType<span class="sy4">;</span>
&#160;
	ReaderType<span class="sy4">::</span><span class="me2">Pointer</span> polyDataReader <span class="sy1">=</span> ReaderType<span class="sy4">::</span><span class="me2">New</span><span class="br0">&#40;</span><span class="br0">&#41;</span><span class="sy4">;</span>
