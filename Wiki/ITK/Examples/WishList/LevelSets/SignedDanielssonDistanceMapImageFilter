  <span class="k">typedef</span> <span class="n">itk</span><span class="o">::</span><span class="n">RescaleIntensityImageFilter</span><span class="o">&lt;</span> <span class="n">FloatImageType</span><span class="p">,</span> <span class="n">UnsignedCharImageType</span> <span class="o">&gt;</span> <span class="n">RescaleFilterType</span><span class="p">;</span>
