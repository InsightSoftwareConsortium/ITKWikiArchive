  <span class="n">SignedDanielssonDistanceMapImageFilterType</span><span class="o">::</span><span class="n">Pointer</span> <span class="n">signedDanielssonDistanceMapImageFilter</span> <span class="o">=</span> <span class="n">SignedDanielssonDistanceMapImageFilterType</span><span class="o">::</span><span class="n">New</span><span class="p">();</span>
  <span class="n">signedDanielssonDistanceMapImageFilter</span><span class="o">-&gt;</span><span class="n">SetInput</span><span class="p">(</span><span class="n">image</span><span class="p">);</span>
